#  1.FIZZBUZZ PROGRAM
     

       #include<stdio.h>
        int main(
        {
        int a,b,c;
         printf("enter the value:",a);
         scanf("%d", &a);
         if (a%3==0)
         printf("fizz\n");
         if (a%5==0)
         printf("buzz\n");
         else
         printf("%d\n",a);
         return 0;


