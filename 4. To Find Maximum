#include<stdio.h>
int max(float x,float y);
int main()
{
        float x,y,z;

        printf("\nEnter The First Value: ");
        scanf("%f",&x);

        printf("\nEnter The Second Value: ");
        scanf("%f",&y);

        z = max(x,y);

        printf("\nMaximum value is: %.2f\n", z);

        return 0;
}
int max(float x,float y)
{
        float result;

        if(x<y)
                result = y;
        else
                result = x;

        return result;
}
